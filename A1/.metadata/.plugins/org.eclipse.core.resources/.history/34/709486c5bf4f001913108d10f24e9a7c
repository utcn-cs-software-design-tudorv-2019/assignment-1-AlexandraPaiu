package Data;

public class Student extends User{

	private int group;

	public Student(int id, String name, String address, String cnp, int group) {
		super(id, cnp, address, name);
		this.group = group;
	}
	
	public int getGroup() {
		return group;
	}

	public void setGroup(int group) {
		this.group = group;
	}
	
	
}
