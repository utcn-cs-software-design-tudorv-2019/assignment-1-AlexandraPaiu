package Data;

public class User {

	private int id;
	private String cnp;
	private String address;
	private String name;
	
	public User(int id, String cnp, String address, String name) {
		this.id = id;
		this.address = address;
		this.cnp = cnp;
		this.name = name;
	}
	
	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getCnp() {
		return cnp;
	}

	public void setCnp(String cnp) {
		this.cnp = cnp;
	}

	public String getAddress() {
		return address;
	}

	public void setAddress(String address) {
		this.address = address;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
	
}
